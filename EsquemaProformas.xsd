      <xsd:element name="importe" type="FLOAT_TYPE"/>
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="SUBVENCION_TYPE">
    <xsd:all>
      <xsd:element name="descripcion" type="STRING50_TYPE" minOccurs="0"/>
      <xsd:element name="porcentaje" type="PORCENTAJE_TYPE" minOccurs="0"/>
      <xsd:element name="importe" type="FLOAT_TYPE" minOccurs="0"/>
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="ANTICIPO_TYPE">
    <xsd:all>
      <xsd:element name="descripcion" type="STRING50_TYPE" minOccurs="0"/>
      <xsd:element name="fecha" type="FECHA_TYPE" minOccurs="0"/>
      <xsd:element name="importe" type="FLOAT_TYPE" minOccurs="0"/>
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="SUPLIDO_TYPE">
