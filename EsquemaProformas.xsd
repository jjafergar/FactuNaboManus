      <xsd:element name="numero_factura" type="STRING10_TYPE" minOccurs="0"/>
      <xsd:element name="serie_factura" type="STRING10_TYPE" minOccurs="0"/>
      <xsd:element name="importe" type="FLOAT_TYPE" minOccurs="0"/>
      <xsd:element name="emisor_tipo_persona" type="TIPO_PERSONA_TYPE" minOccurs="0"/>
      <xsd:element name="emisor_tipo_residencia" type="TIPO_RESIDENCIA_TYPE" minOccurs="0"/>
      <xsd:element name="emisor_tipo_documento" type="TIPO_DOCUMENTO_TYPE" minOccurs="0"/>
      <xsd:element name="emisor_numero_documento" type="STRING9_TYPE" minOccurs="0"/>
