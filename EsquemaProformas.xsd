<?xml version="1.0" ?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <!-- NIVEL 0 -->
  <xsd:element name="proformas" type="PROFORMAS_TYPE"/>
  <xsd:complexType name="PROFORMAS_TYPE">
    <xsd:sequence>
      <xsd:element name="proforma" type="PROFORMA_TYPE" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="PROFORMA_TYPE">
    <xsd:all>
      <xsd:element name="external_id" type="STRING60_TYPE" minOccurs="0"/>
      <xsd:element name="serie_factura" type="STRING10_TYPE"/>
      <xsd:element name="ejercicio" type="STRING4_TYPE"/>
      <xsd:element name="fecha_emision" type="FECHA_TYPE"/>
      <xsd:element name="fecha_vencimiento" type="FECHA_TYPE"/>
      <xsd:element name="fecha_operacion" type="FECHA_TYPE" minOccurs="0"/>
      <xsd:element name="periodo_fecha_inicio" type="FECHA_TYPE" minOccurs="0"/>
      <xsd:element name="periodo_fecha_fin" type="FECHA_TYPE" minOccurs="0"/>
      <xsd:element name="referencia" type="STRING50_TYPE" minOccurs="0"/>
      <!-- Descripción general: Descripción breve de lo que se está facturando. Campo obligatorio exigido por la AEAT -->
      <xsd:element name="descripcion_general" type="STRING500_TYPE"/>
      <xsd:element name="tipo_factura" type="TIPO_FACTURA_TYPE"/>
      <xsd:element name="factura_simplificada_art_7273" type="BOOL_TYPE" minOccurs="0"/>
      <xsd:element name="factura_simplificada_art_61d" type="BOOL_TYPE" minOccurs="0"/>
      <xsd:element name="factura_rectificativa" type="BOOL_TYPE" minOccurs="0"/>
      <xsd:element name="factura_rectificativa_numero" type="STRING20_TYPE" minOccurs="0"/>
      <xsd:element name="factura_rectificativa_ejercicio" type="STRING4_TYPE" minOccurs="0"/>
      <xsd:element name="factura_rectificativa_fecha_emision" type="FECHA_TYPE" minOccurs="0"/>
      <xsd:element name="factura_rectificativa_codigo" type="RECTIFICATIVA_MOTIVO_CODIGO_TYPE" minOccurs="0"/>
      <xsd:element name="factura_rectificativa_tipo" type="TIPO_RECTIFICATIVA_TYPE" minOccurs="0"/>
      <xsd:element name="cliente" type="CLIENTE_TYPE"/>    
      <!-- Permitir email y afc: '1' para permitir enviar la fatura por email. '0' para no permitirlo   -->
      <xsd:element name="permitir_email_y_afc" type="BOOL_TYPE" minOccurs="0"/>
      <xsd:element name="conceptos" type="CONCEPTOS_TYPE"/>
      <!-- Conceptos Textos: Textos que se pueden intercalar entre los conceptos -->
      <xsd:element name="conceptos_textos" type="CONCEPTOS_TEXTOS_TYPE" minOccurs="0"/>
      <xsd:element name="importe_bruto" type="FLOAT_TYPE"/>
      <xsd:element name="descuentos" type="DESCUENTOS_TYPE" minOccurs="0"/>
      <xsd:element name="cargos" type="CARGOS_TYPE" minOccurs="0"/>
      <!-- Total Importe Bruto = Importe Bruto - Total Descuentos + Total Cargos -->
      <xsd:element name="total_importe_bruto" type="FLOAT_TYPE"/>
      <xsd:element name="impuestos_repercutidos" type="IMPUESTOS_REPERCUTIDOS_TYPE" minOccurs="0"/>
      <xsd:element name="impuestos_retenidos" type="IMPUESTOS_RETENIDOS_TYPE" minOccurs="0"/>
      <!-- Importe Total = Total Importe Bruto + Total Impuestos Repercutidos - Total Impuestos Retenidos -->
      <xsd:element name="importe_total" type="FLOAT_TYPE"/>
      <xsd:element name="subvenciones" type="SUBVENCIONES_TYPE" minOccurs="0"/>
      <xsd:element name="anticipos" type="ANTICIPOS_TYPE" minOccurs="0"/>
      <xsd:element name="suplidos" type="SUPLIDOS_TYPE" minOccurs="0"/>
      <xsd:element name="gastos_financieros" type="GASTOS_FINANCIEROS_TYPE" minOccurs="0"/>
      <xsd:element name="retencion" type="RETENCION_TYPE" minOccurs="0"/>
      <xsd:element name="pagos_especie" type="PAGOS_ESPECIE_TYPE" minOccurs="0"/>
      <xsd:element name="forma_pago" type="FORMA_PAGO_TYPE"/>
      <xsd:element name="total_descuentos" type="FLOAT_TYPE" minOccurs="0"/>
      <xsd:element name="total_cargos" type="FLOAT_TYPE" minOccurs="0"/>
      <xsd:element name="total_impuestos_repercutidos" type="FLOAT_TYPE" minOccurs="0"/>
      <xsd:element name="total_impuestos_retenidos" type="FLOAT_TYPE" minOccurs="0"/>
      <xsd:element name="total_subvenciones" type="FLOAT_TYPE" minOccurs="0"/>
      <xsd:element name="total_anticipos" type="FLOAT_TYPE" minOccurs="0"/>
      <!-- Total a pagar = Importe Total - Total Subvenciones - Total Anticipos -->
      <xsd:element name="total_a_pagar" type="FLOAT_TYPE"/>
      <xsd:element name="total_suplidos" type="FLOAT_TYPE" minOccurs="0"/>
      <xsd:element name="total_gastos_financieros" type="FLOAT_TYPE" minOccurs="0"/>
      <xsd:element name="total_retencion" type="FLOAT_TYPE" minOccurs="0"/>
      <xsd:element name="total_pagos_especie" type="FLOAT_TYPE" minOccurs="0"/>
      <!-- Total a ejecutar = Total A Pagar - Total Retención - Total Pagos Especie + Total Suplidos + Total Gastos Financieros -->
      <xsd:element name="total_a_ejecutar" type="FLOAT_TYPE"/>
      <xsd:element name="literales_legales" type="STRING2500_TYPE" minOccurs="0"/>
      <xsd:element name="informacion_adicional" type="STRING2500_TYPE" minOccurs="0"/>
      <xsd:element name="texto_libre" type="STRING500_TYPE" minOccurs="0"/>
      <xsd:element name="plantilla_facturas_emitidas" type="STRING100_TYPE" minOccurs="0"/>
      <xsd:element name="plantilla_facturas_proforma" type="STRING100_TYPE" minOccurs="0"/>
    </xsd:all>
  </xsd:complexType>
  <!-- NIVEL 5 -->
  <xsd:complexType name="FORMA_PAGO_TYPE">
    <xsd:all>
      <xsd:element name="metodo" type="METODO_PAGO_TYPE"/>
      <!-- Si la forma de pago es 'transferencia', todos los campos referidos a esta son obligatorios -->
      <xsd:element name="transferencia_banco" type="STRING50_TYPE" minOccurs="0"/>
      <xsd:element name="transferencia_beneficiario" type="STRING50_TYPE" minOccurs="0"/>
      <xsd:element name="transferencia_concepto" type="STRING50_TYPE" minOccurs="0"/>
      <xsd:element name="transferencia_iban" type="STRING24_TYPE" minOccurs="0"/>
      <xsd:element name="transferencia_bic" type="STRING12_TYPE" minOccurs="0"/>
      <!-- Si la forma de pago es 'domiciliacion', todos los campos referidos a esta son obligatorios -->
      <xsd:element name="domiciliacion_descripcion" type="STRING50_TYPE" minOccurs="0"/>
      <xsd:element name="domiciliacion_tipo_pago" type="DOMICILIACION_TIPO_PAGO_TYPE" minOccurs="0"/>
      <xsd:element name="domiciliacion_bic" type="STRING12_TYPE" minOccurs="0"/>
      <xsd:element name="domiciliacion_iban" type="STRING24_TYPE" minOccurs="0"/>
      <xsd:element name="domiciliacion_mandato_fecha" type="FECHA_TYPE" minOccurs="0"/>
      <xsd:element name="domiciliacion_mandato_id" type="STRING35_TYPE" minOccurs="0"/>
      <xsd:element name="domiciliacion_numero_documento" type="STRING20_TYPE" minOccurs="0"/>
      <xsd:element name="domiciliacion_tipo_documento" type="TIPO_DOCUMENTO_TYPE" minOccurs="0"/>
      <xsd:element name="domiciliacion_titular" type="STRING50_TYPE" minOccurs="0"/>
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="CLIENTE_TYPE">
    <xsd:all>
      <xsd:element name="tipo_persona" type="TIPO_PERSONA_TYPE"/>
