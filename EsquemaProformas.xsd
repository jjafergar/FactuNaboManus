    </xsd:all>
  </xsd:complexType>
  <!-- NIVEL 6 -->
  <xsd:complexType name="CONCEPTO_TYPE">
    <xsd:all>
      <xsd:element name="descripcion" type="STRING500_TYPE"/>
      <xsd:element name="categoria" type="STRING20_TYPE" minOccurs="0"/>
      <xsd:element name="cuenta_contable" type="STRING15_TYPE" minOccurs="0"/>
      <xsd:element name="descripcion_larga" type="STRING2500_TYPE" minOccurs="0"/>
      <xsd:element name="unidad_medida" type="UNIDAD_MEDIDA_TYPE"/>
      <xsd:element name="unidades" type="FLOAT_TYPE"/>
      <xsd:element name="base_unidad" type="FLOAT_TYPE"/>
      <!-- Importe Bruto = Unidades * Base Unidad -->
      <xsd:element name="importe_bruto" type="FLOAT_TYPE"/>
      <!-- Base Imponible = Importe Bruto - Total descuentos + Total Cargos -->
      <xsd:element name="base_imponible" type="FLOAT_TYPE"/>
      <!-- Importe Total = Base Imponible + Total Impuestos Repercutidos - Total Impuestos Retenidos -->
      <xsd:element name="importe_total" type="FLOAT_TYPE"/>
      <xsd:element name="informacion_adicional" type="STRING2500_TYPE" minOccurs="0"/>
      <xsd:element name="descuentos" type="DESCUENTOS_TYPE" minOccurs="0"/>
      <xsd:element name="cargos" type="CARGOS_TYPE" minOccurs="0"/>
      <xsd:element name="impuestos_repercutidos" type="IMPUESTOS_REPERCUTIDOS_TYPE" minOccurs="0"/>
      <xsd:element name="impuestos_retenidos" type="IMPUESTOS_RETENIDOS_TYPE" minOccurs="0"/>
      <xsd:element name="total_descuentos" type="FLOAT_TYPE" minOccurs="0"/>
      <xsd:element name="total_cargos" type="FLOAT_TYPE" minOccurs="0"/>
      <xsd:element name="total_impuestos_repercutidos" type="FLOAT_TYPE" minOccurs="0"/>
      <xsd:element name="total_impuestos_retenidos" type="FLOAT_TYPE" minOccurs="0"/>
      <xsd:element name="fiscalidad_especial" type="FISCALIDAD_ESPECIAL_TYPE" minOccurs="0"/>
      <xsd:element name="literales_legales" type="STRING2500_TYPE" minOccurs="0"/>
      <xsd:element name="referencias_emisor" type="REFERENCIAS_EMISOR_TYPE" minOccurs="0"/>
      <xsd:element name="referencias_receptor" type="REFERENCIAS_RECEPTOR_TYPE" minOccurs="0"/>
