      <xsd:element name="otras_referencias" type="OTRAS_REFERENCIAS_TYPE" minOccurs="0"/>
      <xsd:element name="referencias_albaranes" type="REFERENCIAS_ALBARANES_TYPE" minOccurs="0"/>
