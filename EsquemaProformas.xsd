      <xsd:element name="apellidos" type="STRING50_TYPE" minOccurs="0"/>
      <xsd:element name="nombre_comercial" type="STRING50_TYPE" minOccurs="0"/>
      <xsd:element name="tipo_documento" type="TIPO_DOCUMENTO_TYPE"/>
      <xsd:element name="numero_documento" type="STRING20_TYPE"/>
      <xsd:element name="cuenta_contable" type="STRING15_TYPE" minOccurs="0"/>
      <xsd:element name="cuenta_contable_suplidos" type="STRING15_TYPE" minOccurs="0"/>
      <xsd:element name="observacion" type="STRING200_TYPE" minOccurs="0"/>
      <xsd:element name="tipo_residencia" type="TIPO_RESIDENCIA_TYPE"/>
      <xsd:element name="codigo_pais" type="CODIGO_PAIS_TYPE"/>
      <xsd:element name="domicilio" type="STRING50_TYPE"/>
      <xsd:element name="domicilio_2" type="STRING50_TYPE" minOccurs="0"/>
      <xsd:element name="poblacion" type="STRING50_TYPE"/>
      <xsd:element name="provincia" type="STRING20_TYPE"/>
      <xsd:element name="cp" type="STRING5_TYPE"/>
      <xsd:element name="cp_2" type="STRING10_TYPE" minOccurs="0"/>
      <xsd:element name="telefono" type="STRING15_TYPE" minOccurs="0"/>
      <xsd:element name="email" type="STRING100_TYPE" minOccurs="0"/>
      <xsd:element name="persona_contacto" type="STRING50_TYPE" minOccurs="0"/>
      <xsd:element name="notas" type="STRING500_TYPE" minOccurs="0"/>
      <xsd:element name="emails_extra" type="EMAILS_EXTRA_TYPE" minOccurs="0"/>
      <xsd:element name="domiciliaciones" type="DOMICILIACIONES_TYPE" minOccurs="0"/>
      <xsd:element name="centros_administrativos_face" type="CENTROS_ADMINISTRATIVOS_FACE_TYPE" minOccurs="0"/>
      <!-- Los nodos descritos a continuación están obsoletos. Se mantienen por compatibilidad con versiones anteriores. En su lugar, usar el nodo CENTROS_ADMINISTRATIVOS_FACE_TYPE -->
      <xsd:element name="oficina_contable_descripcion" type="STRING40_TYPE" minOccurs="0"/>
      <xsd:element name="oficina_contable_codigo" type="STRING11_TYPE" minOccurs="0"/>
      <xsd:element name="oficina_contable_domicilio" type="STRING50_TYPE" minOccurs="0"/>
      <xsd:element name="oficina_contable_poblacion" type="STRING50_TYPE" minOccurs="0"/>
      <xsd:element name="oficina_contable_provincia" type="STRING20_TYPE" minOccurs="0"/>
      <xsd:element name="oficina_contable_cp" type="STRING5_TYPE" minOccurs="0"/>
      <xsd:element name="organo_gestor_descripcion" type="STRING40_TYPE" minOccurs="0"/>
      <xsd:element name="organo_gestor_codigo" type="STRING11_TYPE" minOccurs="0"/>
      <xsd:element name="organo_gestor_domicilio" type="STRING50_TYPE" minOccurs="0"/>
      <xsd:element name="organo_gestor_poblacion" type="STRING50_TYPE" minOccurs="0"/>
      <xsd:element name="organo_gestor_provincia" type="STRING20_TYPE" minOccurs="0"/>
