      <xsd:element name="unidad_tramitadora_descripcion" type="STRING40_TYPE" minOccurs="0"/>
      <xsd:element name="unidad_tramitadora_codigo" type="STRING11_TYPE" minOccurs="0"/>
      <xsd:element name="unidad_tramitadora_domicilio" type="STRING50_TYPE" minOccurs="0"/>
      <xsd:element name="unidad_tramitadora_poblacion" type="STRING50_TYPE" minOccurs="0"/>
      <xsd:element name="unidad_tramitadora_provincia" type="STRING20_TYPE" minOccurs="0"/>
      <xsd:element name="unidad_tramitadora_cp" type="STRING5_TYPE" minOccurs="0"/>
      <xsd:element name="organo_proponente_descripcion" type="STRING40_TYPE" minOccurs="0"/>
      <xsd:element name="organo_proponente_codigo" type="STRING11_TYPE" minOccurs="0"/>
      <xsd:element name="organo_proponente_domicilio" type="STRING50_TYPE" minOccurs="0"/>
      <xsd:element name="organo_proponente_poblacion" type="STRING50_TYPE" minOccurs="0"/>
      <xsd:element name="organo_proponente_provincia" type="STRING20_TYPE" minOccurs="0"/>
      <xsd:element name="organo_proponente_cp" type="STRING5_TYPE" minOccurs="0"/>
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="CONCEPTOS_TYPE">
    <xsd:sequence>
      <xsd:element name="concepto" type="CONCEPTO_TYPE" maxOccurs="unbounded"/>
